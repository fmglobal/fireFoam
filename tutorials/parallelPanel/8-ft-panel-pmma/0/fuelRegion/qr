FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/pyrolysisRegion";
    object      qr;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    fuel_top
    {
        type            zeroGradient;
    }

    region0_to_fuelRegion_fuel
    {
        type            mappedField;
        sampleRegion    fuelRegion;
        sampleMode      nearestPatchFace;
        samplePatch     region0_to_fuelRegion_fuel;
        flipNormals     yes;
        offset          (0 0 0);
        fieldName       QrNet;
        setAverage      no;
        average         0;
        value           uniform 0;
    }
}
