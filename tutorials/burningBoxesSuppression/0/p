FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    ground
    {
        type            calculated;
        value           $internalField;
    }

    outlet
    {
        type            calculated;
        value           $internalField;
    }

    sideOpen
    {
        type            calculated;
        value           $internalField;
    }

    burner
    {
        type            calculated;
        value           $internalField;
    }

    "region0_to_solidFuel.*"
    {
        type            calculated;
        value           $internalField;
    }
}

